<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800" prefWidth="1280" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Game.Interface.InterfaceController">
   <children>
      <Canvas fx:id="canvas" height="520.0" layoutY="43.0" width="1280.0" />
      <HBox fx:id="controls" prefHeight="43.0" prefWidth="1280.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#StartPress" prefHeight="40.0" prefWidth="150.0" text="Start">
               <font>
                  <Font name="Courier New" size="22.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Options">
               <font>
                  <Font name="Courier New" size="22.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Menu">
               <font>
                  <Font name="Courier New" size="22.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#QuitPress" prefHeight="40.0" prefWidth="150.0" text="Quit">
               <font>
                  <Font name="Courier New" size="22.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <AnchorPane id="controls" fx:id="controls" layoutY="565.0" prefHeight="235.0" prefWidth="1280.0" />
